prim I
prim left
prim right
prim invol

struct Path (A : Pi I -> Type) (a : A left) (b : A right) : Set
  | at (i : I) : A i { | left => a
                       | right => b }

def `≡` Eq {A : Set} (a b : A) : Set
  => Path (\ i => A) a b

def idp {A : Set} (a : A) : a ≡ a
  => new Path A a a { | at i => a }

def test : Eq (invol left) right
  => idp _
