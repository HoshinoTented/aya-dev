prim I
prim Partial
prim intervalMin
prim intervalMax
prim invol

def infix /\ => intervalMin
tighter \/

def infix \/ => intervalMax

def ~ => invol

def p1 (A : Type) (a : A) (i : I) : Partial (~ i) A =>
  {| ~ i := a |}
def p2 (A : Type) (b : A) (j : I) : Partial (~ j) A =>
  {| ~ j := b |}

def cmp {A : Type} (x : A)
  : [| i j |] (Partial (~ j) A) {| ~ i := p1 A x j |}
  => \i => p2 A x i
