open import Paths

open class Squash
  | A : Type
  | value: A

open data Squash2
  | A : Type
  | squash A

def squashElim {A: Type} {P: Prop} (f: A -> P) (squash: Squash A): P => f squash.value

def squash2Elim {A: Type} {P: Prop} (f: A -> P) (squash: Squash2 A): P
  | f, (squash a) => f a

open data Carry (A : Prop) : Type
| carry A

open data Bool : Prop | true | false

def check : carry true = carry false => idp
