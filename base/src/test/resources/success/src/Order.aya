open import Arith::Nat
open import Data::Bool

def odd Nat : Bool
  | zero => false
  | suc n => even n

def even Nat : Bool
  | zero => true
  | suc n => odd n

open data Rose (A : Type) : Type
  | Node A (Forest A)

open data Forest (A : Type) : Type
  | Empty
  | Cons (Rose A) (Forest A)

def ack Nat Nat : Nat
  | zero, n      => suc n
  | suc m, zero  => ack m (suc zero)
  | suc m, suc n => ack m (ack (suc m) n)

def f Nat : Nat
  | zero => zero
  | suc n => g (suc n)

def g Nat : Nat
  | zero => zero
  | suc n => f n

