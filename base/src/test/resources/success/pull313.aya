open data Nat : Set 0
 | zero
 | suc Nat

data Fin (n : Nat) : Set 0
 | suc m => fzero
 | suc m => fsuc (Fin m)

def cover1 (f : Fin zero) : Nat
 | impossible

def cover2 (f : Fin (suc zero)) : Nat
 | fzero => zero
 | fsuc impossible

def finToNat (n : Nat) (att : Fin n) : Nat
 | zero, impossible
 | suc n, fzero => zero
 | suc n, fsuc a => suc (finToNat n a)
