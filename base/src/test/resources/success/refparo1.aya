-- #2 in public repo
data Entity : Set
  | john

struct EventT : Set 1
  | agentT : Set 0

struct Event (t : EventT) : Set 1
  | agent : t.agentT

data Run (e : Event (new EventT { | agentT => Entity })) : Set
  | (john) => run1

def mkVerb {t : EventT} (p : Event t -> Set 0) : (Event t -> Set 0) -> Set 1
  => \f => Sig (e : Event t) ** (Sig (p e) ** (f e))
