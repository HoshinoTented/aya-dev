open data Nat : Type | zero | suc Nat
open data Bool : Type | true | false

def odd Nat : Bool
  | zero => false
  | suc n => even n

def even Nat : Bool
  | zero => true
  | suc n => odd n

open data Rose (A : Type) : Type
  | Node A (Forest A)

open data Forest (A : Type) : Type
  | Empty
  | Cons (Rose A) (Forest A)

