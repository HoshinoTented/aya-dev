open import Paths
open import Arith::Int

public open data S¹
| base
| loop (i : I) { ∂(i) := base }

def ΩS¹ => base = base
def ploop : ΩS¹ => \i => loop i
def intLoop Int : ΩS¹
| signed true (suc n) => intLoop (pos n) <==> ploop
| signed false (suc n) => intLoop (neg n) <==> sym ploop
| signed _ 0 => idp
| posneg i => idp
