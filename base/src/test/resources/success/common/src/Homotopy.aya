open import Paths

data S1 : Type
| base
| loop I { | left => base | right => base }

data Seg : Type
| p1 | p2
| loop I { | left => p1 | right => p2 }

data Torus : Type
| point
| line1 I { | left => point | right => point }
| line2 I { | left => point | right => point }
| face I I { | left, i => line2 i
             | right, i => line2 i
             | i, left => line1 i
             | i, right => line1 i }

def idpath {A : Type} (a : A) (i : I) : A
 | a, i => a
